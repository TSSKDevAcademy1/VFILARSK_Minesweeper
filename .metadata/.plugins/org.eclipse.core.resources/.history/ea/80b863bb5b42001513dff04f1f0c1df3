package core;

public class Clue extends Tile{
	private int value =0;

	public Clue(int positionX, int positionY, int value) {
		super(positionX, positionY);
		this.value = value;
		// TODO Auto-generated constructor stub
	}

	public int getValue() {
		return value;
	}

	public void setValue(int value) {
		this.value = value;
	}

	
}
