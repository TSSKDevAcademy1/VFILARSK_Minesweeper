package core;

public class EmptyClue extends Tile {

	private int value = 5;
	public EmptyClue(int positionX, int positionY) {
		super(positionX, positionY);
		// TODO Auto-generated constructor stub
	}
	public int getValue() {
		return value;
	}
	public void setValue(int value) {
		this.value = value;
	}

	

}
