package microunit;

public class Assert {

	public static void assertEquals(Object current, Object expected) {
		if (current == null) {
			if (expected != null) {
				System.err.println("Error, not equals");
				return;
			}
		} else {
			if(expected == null){
				System.err.println("Error,not equals!");
				return;
			}
			
			if(!current.equals(expected)){
				System.err.println("Error,not equals!");
				return;
			}
		}
	}
}
